{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Run Jasmine Tests",
        "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
        "args": [
          "--config=${workspaceFolder}/spec/support/jasmine.json"
        ],
        "outFiles": [
          "${workspaceFolder}/dist/**/*.js"
        ]
      }
    ]
  }